{
  "name": "url-shortner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "test": "vitest --reporter=verbose",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:deploy": "npx prisma migrate deploy && npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name",
    "prisma:push": "npx prisma db push && npx prisma generate",
    "prisma:setup": "prisma generate && prisma migrate deploy && prisma db push",
    "prisma:studio": "npx prisma studio",
    "prisma:wipe": "npx prisma migrate reset --force && npx prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "nanoid": "^5.0.9",
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.50.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^20",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-check-file": "^2.8.0",
    "happy-dom": "^16.7.3",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.2.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.4"
  }
}
